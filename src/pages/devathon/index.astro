---
import DevathonInfo  from "../../components/astro/DevathonInfo/index.astro";
import Layout from "../../layouts/Layout.astro";
import "./styles.css";
import { devathon } from "../../data/devathon";
import { faqs } from "../../data/faqs";
import DevathonFaqs from "../../components/astro/DevathonFaqs/index.astro";
import Slider from "../../components/astro/Slider/index.astro";
import { testimonials } from "../../data/testimonials";
---

<Layout title="Devathon">
  <main>
    <section class="title-container">
      <h1>👨‍💻 Devathon</h1>
    </section>
    <div class="container">
      <DevathonInfo devathon={devathon} />
      <section class="testimonials-container">
        <h1>Testimonios de los participantes</h1>
        <Slider testimonials={testimonials}/>
      </section>
      <section>
        <h1>FAQ</h1>
      </section>
      <DevathonFaqs faqs={faqs} />
    </div>
  </main>
</Layout>