---
import DevathonInfo  from "../../components/astro/DevathonInfo/index.astro";
import Layout from "../../layouts/Layout.astro";
import styles from "./index.module.css";
import { devathon } from "../../data/devathon";
import { faqs } from "../../data/faqs";
import DevathonFaqs from "../../components/astro/DevathonFaqs/index.astro";
---

<Layout title="Devathon">
  <main>
    <section class={styles.titleContainer}>
      <h1>👨‍💻 Devathon</h1>
    </section>
    <div class={styles.container}>
      <!-- DEVATHON INFORMATION -->
      <DevathonInfo devathon={devathon} />
      <section>
        <h1>FAQ</h1>
      </section>
      <!-- FAQ -->
      <DevathonFaqs faqs={faqs} />
    </div>
  </main>
</Layout>